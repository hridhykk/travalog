{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "module": "CommonJS",
    "target": "ES2020",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./src",
    "types": ["node","express", "cookie-parser", "jsonwebtoken"],
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}